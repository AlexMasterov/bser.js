{
  "name": "fast-bser",
  "version": "0.4.6",
  "description": "A fast and memory-efficient BSER serialization library",
  "keywords": [
    "bser",
    "binary",
    "serialization"
  ],
  "license": "MIT",
  "author": "Alex Masterov <alex.masterow@gmail.com>",
  "repository": "AlexMasterov/fast-bser",
  "homepage": "https://github.com/AlexMasterov/fast-bser.js#readme",
  "bugs": {
    "url": "https://github.com/AlexMasterov/fast-bser.js/issues"
  },
  "main": "./src/index.js",
  "files": [
    "src/"
  ],
  "engines": {
    "node": ">=8.3.0"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "nyc mocha --timeout 0 test/*.test.js",
    "test-report": "nyc --reporter=html --reporter=text mocha test/*.test.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "ascii-chr": "0.3.0",
    "utf8-bin": "0.3.1"
  },
  "devDependencies": {
    "coveralls": "3.0.2",
    "mocha": "5.2.0",
    "nyc": "13.1.0",
    "rollup": "0.67.3"
  },
  "nyc": {
    "all": false,
    "cache": true,
    "sourceMap": true,
    "instrument": true,
    "reporter": [],
    "include": [
      "src/**/*.js"
    ]
  }
}
